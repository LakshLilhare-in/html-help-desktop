{
  "name": "html-help-desktop",
  "productName": "HTML Help Desktop",
  "version": "0.1.0",
  "description": "HTML Help is now available in desktop mode. Made using Electron",
  "main": "main.js",
  "author": "Laksh Lilhare",
  "scripts": {
    "start": "electron-forge start",
    "dist": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "electron": "^8.0.2"
  },
  "build": {
    "appId": "com.electron.app",
    "win": {
      "target": "portable"
    },
    "portable": {
      "unicode": false,
      "artifactName": "my_electron_app.exe"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LakshLilhare-in/htmlhelp-desktop.git"
  },
  "keywords": [
    "Elegant",
    "Fast",
    "Easy-to-use"
  ],
  "license": "SEE LICENSE IN license",
  "bugs": {
    "url": "https://github.com/LakshLilhare-in/htmlhelp-desktop/issues"
  },
  "homepage": "https://github.com/LakshLilhare-in/htmlhelp-desktop#readme",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
